{
  "name": "webbox",
  "version": "1.0.0",
  "description": "Webbased course editor powered by sourcebox",
  "main": "webbox.js",
  "scripts": {
    "clean": "rimraf public/js/* public/css/*",
    "clean-react": "rimraf node_modules/spectacle/node_modules/component-playground/node_modules/react-dom && rimraf node_modules/spectacle/node_modules/react",
    "test:build": "webpack --config webpack.config.test.js",
    "test:watch": "webpack --config webpack.config.test.js --watch",
    "test": "lab --assert code --coverage --coverage-path /lib -v -I 'Reflect,core,_babelPolyfill,regeneratorRuntime,__core-js_shared__,System,asap,Observable",
    "start:prod": "cross-env NODE_ENV=production node webbox.babel.js",
    "start:dev": "nodemon webbox.babel.js",
    "web:build": "npm run clean-react && webpack --config webpack.config.production.js -p --progress",
    "web:json:": "webpack --config webpack.config.production.js -p --progress --json > webox.stats.json",
    "web": "npm run clean-react && webpack --watch --hide-modules"
  },
  "private": true,
  "engine": ">= node 8.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ebertmi/webbox.git"
  },
